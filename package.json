{
  "name": "dcp-web",
  "version": "0.0.1",
  "description": "Developer Community Platform WEB",
  "author": "Dec0dex",
  "homepage": "https://decodex.net",
  "license": "GPL-3.0-only",
  "contributors": [
    "Dejan Radmanovic <office@decodex.net>"
  ],
  "keywords": [
    "angular",
    "angular 18",
    "jest",
    "transloco",
    "prettier",
    "cypress",
    "vuepress",
    "docker",
    "eslint",
    "storybook"
  ],
  "repository": "https://github.com/dec0dex/dcp-web.git",
  "bugs": {
    "url": "https://github.com/dec0dex/dcp-web/issues"
  },
  "readme": "https://github.com/dec0dex/dcp-web/blob/master/docs/README.md",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:prod:stats": "ng build --configuration production --stats-json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:cov": "jest --coverage",
    "test:e2e": "ng e2e",
    "lint": "eslint src/app/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"cypress/e2e/**/*.ts\"",
    "analyze": "source-map-explorer dist/dcp-web/**/*.js",
    "prepare": "husky || true",
    "audit": "auditjs ossi",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "storybook": "pnpm start-storybook -p 6006",
    "build-storybook": "pnpm && build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/material": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@jsverse/transloco": "^7.4.3",
    "launchdarkly-js-client-sdk": "^3.4.0",
    "rxjs": "~7.8.1",
    "supertokens-web-js": "^0.13.0",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular-eslint/builder": "^18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/schematics": "^18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "18.1.3",
    "@angular/compiler-cli": "^18.2.2",
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@cypress/schematic": "^2.5.2",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/angular": "^8.2.9",
    "@storybook/builder-webpack5": "^8.2.9",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/test": "8.2.7",
    "@types/eslint": "9.6.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "@vuepress/bundler-vite": "^2.0.0-rc.15",
    "@vuepress/plugin-search": "^2.0.0-rc.41",
    "@vuepress/theme-default": "^2.0.0-rc.41",
    "auditjs": "^4.0.45",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^9.1.3",
    "cypress": "latest",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.2.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "source-map-explorer": "^2.5.3",
    "ts-node": "~10.9.2",
    "typescript": "^5.5.4",
    "vue": "^3.4.38",
    "vuepress": "^2.0.0-rc.15",
    "vuepress-plugin-md-enhance": "^2.0.0-rc.52"
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.9.0"
}
